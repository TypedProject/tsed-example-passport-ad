{
  "name": "@tsed/passportjs-example",
  "version": "1.0.0",
  "description": "",
  "main": "server/src/index.js",
  "scripts": {
    "------------------ client --": "",
    "ng": "ng",
    "force": "azureToRebuild",
    "start:client": "ng serve --port 4201",
    "start:client:dev": "ng serve --port 4201 --configuration=dev",
    "build:client": "ng build",
    "test:client": "ng test",
    "lint:client": "ng lint",
    "e2e:client": "ng e2e",
    "------------------ server --": "",
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "nodemon --watch 'server/src/**/*.ts' --ignore 'node_modules/**/*' --exec ts-node src/index.ts",
    "start": "ts-node server/src/index.ts",
    "start:server:watch": "nodemon --watch server/src --exec ts-node server/src/index.ts"
  },
